{
  "name": "verificaarquivos",
  "version": "1.0.0",
  "description": "Verificar integridades dos arquivos de um diretorio especifico",
  "main": "index.js",
  "bin": "build/index.js",
  "pkg": {
    "assets": [
      "build"
    ],
    "targets": [
      "node16"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node build/index.js",
    "build": "tsc --build",
    "install-pacotes": "npm install && npm install pm2 -g && npm install pm2-windows-startup -g && pm2-startup install && pm2 save --force && pm2 delete all & pm2 save --force  && pm2 start build/index.js --name VerificadorMOR && pm2 save --force"
  },
  "author": "NicolasBonnefont",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.3.1",
    "archiver-zip-encrypted": "^1.0.11",
    "axios": "^0.26.1",
    "chokidar": "^3.5.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "execa": "^5.1.0",
    "express": "^4.18.1",
    "filesize": "^8.0.7",
    "pm2": "^5.2.0",
    "pm2-windows-startup": "^1.0.3",
    "ts-node-dev": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@types/archiver": "^5.3.1",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "typescript": "^4.7.4"
  }
}
